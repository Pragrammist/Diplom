@*
   
*@
@{
	@model User;

}

@using (Html.BeginForm("Create", "User", FormMethod.Post))
{
    <p>
        @Html.LabelFor(m => m.Email)
        @Html.TextBoxFor(m => m.Email)
    </p>
    <p>
        @Html.LabelFor(m => m.Password)
        @Html.PasswordFor(m => m.Password)
    </p>
    <p>
        <input type="submit" value="Зарегестрироваться" />
    </p>
}

